<div id='root'></div>

<script type="text/javascript">
  window.pledgeling_processing_fee = parseFloat(<%= ENV['PLEDGELING_PROCESSING_FEE'] %>)
  window.currentUser = 
  <% if current_user %>
  {
    id: "<%= current_user&.id %>",
    email: "<%= current_user&.email %>"
  }
  <% else %>
    null
  <% end %>
  window.apiUrl = '<%= ENV['API_URL'] %>'
  window.authenticityToken = '<%= form_authenticity_token %>'
</script>
<%= javascript_pack_tag 'router' %>
